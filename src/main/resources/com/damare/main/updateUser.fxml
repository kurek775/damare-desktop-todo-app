<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="376.0" prefWidth="364.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.damare.main.UpdateUserController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <children>
        <Label text="uname" />
        <TextField fx:id="nameField" />
        <Label text="email" />
        <TextField fx:id="emailField" />
        <Label text="password" />
        <TextField fx:id="passwordField" />

        <Button fx:id="saveBtn" onAction="#onSaveButtonClick" text="Save" />
        <Button fx:id="homeBtn" onAction="#toHome" text="Home" />

    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>


</VBox>
